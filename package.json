{
  "name": "hakki",
  "version": "2.2.1",
  "description": "An opinionated, modern, and scalable alternative to node_acl.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "coverage": "nyc report",
    "test": "nyc --check-coverage --lines 100 ava --verbose",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashersw/hakki.git"
  },
  "keywords": [
    "acl"
  ],
  "author": "Armagan Amcalar <armagan@amcalar.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashersw/hakki/issues"
  },
  "homepage": "https://github.com/dashersw/hakki#readme",
  "devDependencies": {
    "ava": "^4.0.1",
    "eslint": "^8.7.0",
    "eslint-config-standard": "^17.0.0-0",
    "mongoose": "^6.1.7",
    "nyc": "^15.1.0",
    "randomstring": "^1.2.2"
  },
  "peerDependencies": {
    "mongoose": "^5.2.6 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "mongoose": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "eqeqeq": "off",
      "arrow-parens": [
        2,
        "as-needed"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2022
    }
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "engines": {
    "node": ">=12"
  }
}
